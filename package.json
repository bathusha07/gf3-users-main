{
  "name": "gf3-users",
  "version": "1.0.0",
  "description": "This README would normally document whatever steps are necessary to get your application up and running.",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon src/index.ts",
    "start:devDebug": "nodemon --inspect=0.0.0.0:9229 .",
    "build": "tsc -p .",
    "build:prod": "tsc -p tsconfig.prod.json",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "prisma db seed",
    "lint": "eslint src/ features/",
    "lint:fix": "eslint --fix src/**/*.ts",
    "format:all": "prettier src/ features/ --write",
    "prepare": "husky install",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml -f docker-compose.pubsubemu.yml up",
    "docker:devDebug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up",
    "docker:devMulti": "docker-compose -f docker-compose.yml -f docker-compose.multi.yml up",
    "docker:devGF2": "docker-compose -f docker-compose.yml -f docker-compose.gf2.yml -f docker-compose.pubsubemu.yml up",
    "newman:local": "sh ./.newman/newman_run.sh local",
    "newman:local:all": "sh ./.newman/newman_run_all.sh local",
    "test:component": "cucumber-js --config cucumber.json"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/makegoodfood/gf3-users.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/makegoodfood/gf3-users#readme",
  "dependencies": {
    "@google-cloud/pubsub": "^2.10.0",
    "@makegoodfood/gf3-types": "^0.29.12",
    "@makegoodfood/gf3cf-lib": "^0.6.0",
    "@prisma/client": "^4.7.1",
    "@tsconfig/node14": "^1.0.0",
    "@types/date-and-time": "^0.13.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.40",
    "@types/stripe": "^8.0.417",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@xstate/fsm": "^1.6.0",
    "awilix": "^4.3.3",
    "axios": "^0.21.1",
    "date-and-time": "^1.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^11.10.1",
    "ip-regex": "^4.3.0",
    "newrelic": "^8.5.0",
    "openapi-typescript": "^3.2.3",
    "prisma": "^4.7.1",
    "stripe": "^8.139.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.9.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@types/chai": "^4.3.4",
    "@types/cucumber": "^7.0.0",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/newrelic": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.7",
    "cucumber-tsflow": "^4.0.0-rc.7",
    "eslint": "^7.24.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^2.0.6",
    "lint-staged": "^10.5.4",
    "newman": "^5.2.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "uuid-regexp": "^0.3.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,css,md}": "prettier --write",
    "*.prisma": "prisma format"
  }
}
